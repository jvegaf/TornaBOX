<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="90.0">
         <children>
            <Label style="-fx-padding: 0 10;" text="Library">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <ListView fx:id="libraryListView" />
         </children>
      </VBox>
      <VBox layoutX="10.0" layoutY="10.0">
         <children>
            <Label style="-fx-padding: 0 10;" text="Playlists">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <ListView fx:id="playlistListView"/>
         </children>
      </VBox>
   </children>
</fx:root>
